<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.llmauthentication.mapper.JbJobMapper">

    <resultMap id="BaseResultMap" type="com.example.llmauthentication.pojo.JbJob">
            <id property="jobid" column="jobId" jdbcType="INTEGER"/>
            <result property="jobdesp" column="jobDesp" jdbcType="VARCHAR"/>
            <result property="createtime" column="createTime" jdbcType="TIMESTAMP"/>
            <result property="updatetime" column="updateTime" jdbcType="TIMESTAMP"/>
            <result property="jobname" column="jobName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        jobId,jobDesp,createTime,
        updateTime,jobName
    </sql>
</mapper>
